<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>MinecraftDotnet.Game.Gen</PackageId>
        <Title>Minecraft Gen Game</Title>
        <Description>A gen game mode Minecraft server written as a library.</Description>
        <PackageProjectUrl>https://github.com/Serble/GenDotnet</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Serble/GenDotnet.git</RepositoryUrl>
        <Version>1.0.1</Version>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="MinecraftDotnet.ManagedServer" Version="0.1.2-dev.178" />
    </ItemGroup>

</Project>
